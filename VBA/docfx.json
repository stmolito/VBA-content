{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/Offline-Docs/**"
        ]
      }
    ],
    
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path":"/VBA/breadcrumb/toc.json",
        "ms.suite": "office",
        "ms.technology": "vba",
        "ms.author": "o365devx",
        "author": "o365devx",
        "ms.topic": "reference"
    },
    "fileMetadata": {"ms.prod": 
       { 
         "VBA/Access-VBA/**.md": "access",
         "VBA/Excel-VBA/**.md": "excel",
         "VBA/Outlook-VBA/**.md": "outlook",
         "VBA/PowerPoint-VBA/**.md": "powerpoint",
         "VBA/Project-VBA/**.md": "project",
         "VBA/Publisher-VBA/**.md": "publisher",
         "VBA/Visio-VBA/**.md": "visio",
         "VBA/Word-VBA/**.md": "word",
         "VBA/Language-Reference-VBA/**.md": "office",
         "VBA/Office-F1-Landing/**.md": "office",
         "VBA/Office-Mac-VBA/**.md": "office",
         "VBA/Office-Shared-VBA/**.md": "office"
       }},
     "template": "op.html",
    "dest": "VBA"
  }
}
